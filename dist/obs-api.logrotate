/srv/www/obs/api/log/*log /srv/www/obs/api/log/clock*output {
  compress
  dateext
  maxage 365
  rotate 50
  size +8096k
  notifempty
  missingok
  copytruncate
  create 664 wwwrun www
  su wwwrun www
  sharedscripts
  postrotate
    # Generate analytics if the obs-api-analytics package is installed...
    test -f /usr/bin/obs-goaccess-last-7-days.sh && /usr/bin/obs-goaccess-last-7-days.sh
  endscript
}

